{
    "openapi": "3.0.0",
    "info": {
        "title": "Praticando Swagger",
        "description": "Praticando documentação com swagger dentro do estudo de node js",
        "version": "1.0.0",
        "paths": {
            "/sheets/:userName": {
                "get": {
                    "tags": ["crypto"],
                    "summary": "Returns list of sheets parsed from XLSX file",
                    "description": "Returns list of sheets objects, which were parsed each from XLSX file sheet",
                    "requestBody": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "email": {
                                            "type": "string",
                                            "description": "new user's email address"
                                        },
                                        "name": {
                                            "type": "string",
                                            "description": "new user's name"
                                        }
                                    },
                                    "example": {
                                        "email": "example@example.com",
                                        "name": "fulano"
                                    }
                                }
                            }
                        }
                    },
                    "responses": {
                        "201": {
                            "description": "New user created",
                            "content": {
                                "application/json": {
                                    "schema": {
                                        "type": "object",
                                        "description": "custom User object instace of the user",
                                        "properties": {
                                            
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}